<loglevels>
    <level value="0" name="sqlite" enabled="True" />
    <level value="1" name="netpacket" enabled="True" />
    <level value="2" name="queue" enabled="True" />
    <level value="3" name="conditional" enabled="False" />
    <level value="4" name="module import" function="module" enabled="True" />
    <level value="5" name="network.server" function="netserver" enabled="True" />
    <level value="6" name="network.client" function="netclient" enabled="True" />
    <level value="7" name="network" enabled="False" />
    <level value="8" name="ui" enabled="True" />
    <level value="9" name="fixme" enabled="True" />
    <level value="10" name="not implemented" function="notimplemented" enabled="True" />
    <level value="11" name="debug" enabled="True" />
    <level value="12" name="info" enabled="True" />
    <level value="13" name="not compatible" function="notcompatible" enabled="True" />
    <level value="14" name="warning" color="\033[0;36m" bold="True" enabled="True" />
    <level value="15" name="deprecated" bold="True" enabled="True" />
    <level value="16" name="error" bold="True" enabled="True" />
    <level value="17" name="critical" color="\033[1;33m" bold="True" enabled="True" />
    <level value="18" name="fatal"  color="\033[1;41m" bold="True" enabled="True" />
    <level value="19" name="terminate" color="\033[0;36m" bold="True" enabled="True" />
</loglevels>
