<status>
    <level id="0" string="Waiting" colorFG="black" colorBG="white" />
    <level id="1" string="Hold" colorFG="black" colorBG="yellow" />
    <level id="2" string="Running" colorFG="black" colorBG="blue" />
    <level id="3" string="Done" colorFG="green" colorBG="white" />
    <level id="4" string="Failed" colorFG="black" colorBG="red" />
    <level id="5" string="Paused" colorFG="black" colorBG="orange" />
    <level id="6" string="Cancelled" colorFG="grey" colorBG="white" />
</status>
